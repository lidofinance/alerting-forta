{
  "license": "MIT",
  "scripts": {
    "build": "yarn generate-types && yarn run move-migrations && make gen_proto_prod && tsc",
    "build:docker": "docker build -t l2-unified ./",

    "start:mantle": "yarn ts-node src/mantle/main.ts",
    "start:optimism": "yarn ts-node src/optimism/main.ts",
    "push:mantle": "node push-agent.js mantle",

    "start:mantle:prod": "node src/mantle/main.js",

    "test": "jest --runInBand",

    "postinstall": "yarn generate-types && yarn generate-proto",
    "generate-proto": "make gen_ts && make gen_js",
    "generate-types": "typechain --target=ethers-v5 --out-dir=./src/common/generated ./src/common/abi/*",
    "start": "node start-agent.ts",
    "move-migrations": "mkdir -p dist/common/db/migrations && cp src/common/db/migrations/* dist/common/db/migrations/"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@types/lodash": "^4.14.202",
    "async-mutex": "^0.4.0",
    "bignumber.js": "^9.1.2",
    "dotenv": "^16.4.5",
    "ethers": "^5.5.1",
    "express": "^4.19.2",
    "forta-agent": "^0.1.48",
    "fp-ts": "^2.16.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "prom-client": "^15.1.3",
    "ts-retry": "^4.2.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.0.0",
    "@ethersproject/providers": "^5.0.0",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/nodemon": "^1.19.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "postinstall": "^0.8.0",
    "prettier": "^3.1.0",
    "shelljs": "^0.8.4",
    "sqlite": "5.1.1",
    "sqlite3": "^5.1.7",
    "ts-generator": "^0.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.2"
